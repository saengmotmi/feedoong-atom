{
  "name": "@feedoong/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev:edge": "yarn build && wrangler dev --config wrangler.jsonc --port 8788",
    "preview": "yarn build && react-router-serve build/server/index.js",
    "deploy": "yarn build && wrangler deploy",
    "typecheck": "react-router typegen && tsc -b",
    "cf-typegen": "wrangler types --config wrangler.jsonc worker-configuration.d.ts"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "@react-router/serve": "^7.10.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.10.0",
    "remeda": "^2.30.0",
    "ts-pattern": "^5.8.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.10.0",
    "@types/node": "^22.19.11",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-rsc": "~0.5.17",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.67.0"
  }
}
